@model FH_Kiel_Ticketing_App.Models.SuperVisorTicketViewModel

@{
    ViewBag.Title = "Supervisor - Home";
}

<h2>Home</h2>

<div class="container">
    <div class="row">
        <div class="col-xs-7">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">Projects and Thsis in progress</h3>
                </div>
                <div class="panel-body">
                    RoboRally multiplayer Game 
                </div>
            </div>
        </div>
        <div class="col-xs-5">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">New Proposals</h3>
                </div>
                <div class="panel-body">
                    <table class="table table-striped table-hover font14">
                        <thead>
                            <tr>
                                <th>ticket title</th>
                                <th>date created </th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.tickets)
        {
                    <tr>
                        <td><a href="/Ticket/Ticket/@item.recordID">@item.title</a></td>
                        <td>@item.creationDate</td>



                    </tr>

}

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>